{
  "name": "react-starter-app",
  "version": "0.1.0",
  "private": false,
  "main": "dist/index.js",
  "description": "React + Redux + SASS + Eslint + Webpack starter app",
  "keywords": ["react", "redux", "sass", "eslint", "webpack"],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "lint": "eslint --ignore-path .gitignore . --ext js,jsx",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run lint && npm run clean",
    "build": "cross-env NODE_ENV=production webpack --mode production"
  },
  "dependencies": {
    "cross-env": "^5.1.6",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint-config-mmontes11-react": "^1.0.3",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3"
  }
}
